{
  "name": "@nexon-game-event-system-mono/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:all": "nx run-many --target=serve --all",
    "build:apps": "nx run-many --target=build --all",
    "build:libs": "nx run-many --target=build:libs --all",
    "start:gateway": "nx run gateway:serve",
    "start:auth": "nx run auth:serve",
    "start:event": "nx run event:serve",
    "network:create": "./scripts/docker/docker-network-manager.sh create",
    "network:remove": "./scripts/docker/docker-network-manager.sh remove",
    "compose:up": "pnpm network:create && docker compose -f ./scripts/docker/docker-compose.infra.yml -p event-system-container up ",
    "compose:down": "docker compose -p event-system-container down && pnpm network:remove",
    "compose:downv": "docker compose -p event-system-container down -v && pnpm network:remove",
    "k6:init": "./scripts/k6/initialize.sh"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^10.0.2",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.6.0",
    "mongoose": "^8.14.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "20.8.1",
    "@nx/eslint-plugin": "20.8.1",
    "@nx/js": "20.8.1",
    "@nx/nest": "20.8.1",
    "@nx/node": "20.8.1",
    "@nx/web": "20.8.1",
    "@nx/webpack": "20.8.1",
    "@nx/workspace": "20.8.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "~18.16.9",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.13",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
