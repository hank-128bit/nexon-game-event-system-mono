{
  "name": "@nexon-game-event-system-mono/gateway-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "gateway-e2e",
    "implicitDependencies": [
      "gateway"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/gateway-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "gateway:build",
          "gateway:serve"
        ]
      }
    }
  }
}
